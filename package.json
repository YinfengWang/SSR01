{
  "name": "zhydhl",
  "version": "1.0.0",
  "description": "智慧移动护理",
  "main": "index.js",
  "scripts": {
    "build:client": "webpack --config build/webpack.config.client.js",
    "build:server": "webpack --config build/webpack.config.server.js",
    "dev:client": " yarn build:client && yarn  cross-env NODE_ENV=development  webpack serve --open --config build/webpack.config.client.js",
    "dev:server": " cross-env NODE_ENV=development  node server/server.js",
    "clear": "rimraf dist",
    "build": "yarn  clear && yarn  build:client && yarn  build:server",
    "start": "node server/server.js",
    "dev": "yarn run build && yarn run start",
    "lint": "eslint --ext .js --ext .jsx client/",
    "precommit": "yarn lint"
  },
  "author": "MSH",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/plugin-transform-react-display-name": "^7.12.13",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/runtime": "^7.13.10",
    "@hot-loader/react-dom": "^17.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-preset-react-app": "^10.0.0",
    "editorconfig": "^0.15.3",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "4.0.8",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-webpack-plugin": "^2.5.3",
    "html-webpack-plugin": "^5.3.1",
    "http-proxy-middleware": "^1.1.0",
    "husky": "^6.0.0",
    "memory-fs": "^0.5.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.2",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "webpack-merge": "^5.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo 'husky' && yarn lint"
    }
  }
}
